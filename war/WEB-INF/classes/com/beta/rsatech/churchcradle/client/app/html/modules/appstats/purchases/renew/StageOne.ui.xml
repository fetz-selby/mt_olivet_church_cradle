<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.beta.rsatech.churchcradle.client.widgets">
	<ui:style>
		.header-style{height: 95px; padding: 7px;}
	</ui:style>
	<g:HTMLPanel>
		<header class="bg-success no-border {style.header-style}">
			<div class="clearfix">
				
				<div class="text-center">
					<small class="h4 text-muted" ui:field="desc">0 GHS</small>
				</div>
			</div>
			
		</header>

		<div class="panel-body">
			<div role="form">
			
				<div class="form-group">
					<label class="control-label">Select Duration</label>
					<div class="clearfix"></div>
					<div>
						<select style="width:260px" class="chosen-select" ui:field="chosenElement">
							
							<optgroup label="Renew duration">
							    <option value="0">0 month</option>
								<option value="3">3 months</option>
								<option value="6">6 months</option>
								<option value="12">12 months</option>
								<option value="18">18 months</option>
								<option value="24">2 years</option>
							</optgroup>
						</select>
					</div>
				</div>








			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 