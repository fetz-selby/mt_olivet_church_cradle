<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.clearfix {
		height: 0;
		clear: both;
	}
	
	.container {
		padding-bottom: 15px;
	}
	
	.container .infobox {
		float: left;
		width: 100%;
		height: 26px;
	}
	
	.container .positionbox {
		float: left;
		width: 100%;
		height: 26px;
	}
	
	.container .frombox {
		float: left;
		width: 100%;
		height: 26px;
	}
	
	.container .endbox {
		float: left;
		width: 100%;
		height: 26px;
	}
	</ui:style>
	<g:HTMLPanel styleName="panel-body {style.container}">
		<div class="form-group">
			<g:TextBox ui:field="infobox" styleName="form-control"></g:TextBox>
		</div>
		
		<div class="form-group">
			<g:TextBox ui:field="positionbox" styleName="form-control"></g:TextBox>
		</div>
		
		<div class="form-group">
			<g:TextBox ui:field="frombox" styleName="form-control"></g:TextBox>
		</div>
		<div class="form-group">
			<g:TextBox ui:field="endbox" styleName="form-control"></g:TextBox>
		</div>
		<div class="{style.clearfix}"></div>
	</g:HTMLPanel>
</ui:UiBinder> 