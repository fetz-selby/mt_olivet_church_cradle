<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.beta.rsatech.churchcradle.client.widgets">
	<ui:style>
	.scroll-style {
		overflow-y: auto;
		min-height: 265px !important;
	}
	</ui:style>
	<g:HTMLPanel styleName="panel-body">
		<div class="form-group">
			<g:TextBox styleName="form-control" ui:field="dobField"></g:TextBox>
		</div>

		<div class="form-group">
			<label>Other Info</label>
			<c:ReviewPopupTextArea ui:field="otherInfo"></c:ReviewPopupTextArea>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 