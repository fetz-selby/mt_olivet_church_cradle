<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<div class="panel-body">
			<div role="form">
				<div class="form-group">
					<div class="radio">
						<label>
							<input type="radio" name="gender" ui:field="maleInput" />
							Male
						</label>
					</div>
					<div class="radio">
						<label>
							<input type="radio" name="gender" ui:field="femaleInput" />
							Female
						</label>
					</div>
				</div>

				<div class="form-group">
					<g:TextBox styleName="form-control" ui:field="dobField"></g:TextBox>
				</div>

				<div class="form-group">
					<g:TextArea styleName="form-control textarea-style"
						ui:field="addressField"></g:TextArea>
				</div>
				
				<div class="form-group">
					<g:ListBox styleName="form-control" ui:field="eLevelField"></g:ListBox>
				</div>
				
				<div class="form-group">
					<g:TextBox styleName="form-control" ui:field="occupationField"></g:TextBox>
				</div>
				
				<div class="form-group">
					<g:TextBox styleName="form-control" ui:field="employerField"></g:TextBox>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 